<h3>Employees</h3>
<div class="span8">
<table class="table table-condensed">
    <thead>
      <tr>
          <th>Username</th>
          <th>Email</th>
          <th>Registered</th>

      </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
        <td><%= link_to user.name, edit_employee_user_path(user) %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at.to_date %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</div>