<h3>Listing work_time_types</h3>

<table class = "table table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @work_time_types.each do |work_time_type| %>
      <tr>
        <td><%= work_time_type.name %></td>
        <td><%= link_to 'Edit', edit_work_time_type_path(work_time_type), :class => "btn btn-mini btn-primary" %></td>
        <td><%= link_to 'Destroy', work_time_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Work Time Type', new_work_time_type_path %>
